@model List<ShoppingCartItem>;

<div>
    <h1>Indkøbskurv</h1>
    <div>
        @foreach (var item in Model)
        {
            <div id="boardInformation">
                <table>
                    <tr>
                        <th>Id:</th>
                        <th>Product-navn: </th>
                        <th>Mængde:</th>
                    </tr>
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Product.Name</td>
                        <td>@item.Quantity</td>
                    </tr>
                </table>
            </div>
        }
    </div>
</div>



